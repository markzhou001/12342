[
  {
    "description":"模拟一个没有参数的get请求",
    "request":{
      "uri":"/getdemo",
      "method":"get"
    },
    "response":{
      "text":"这是一个没有参数的get请求",
      "headers":{"Content-Type":"text/html;charset=gbk"}
    }
  },

  {
    "description":"模拟一个带参数的get请求",
    "request":{
      "uri":"/getwithparam",
      "method":"get",
      "queries": {
        "name":"zhoujianhua",
        "age": "20"
      }
    },
    "response":{
      "json":{
        "ret": 0,
        "msg": "请求成功",
        "data": {
          "name": "周建华",
          "age": 18
        }
      },
      "headers":{"Content-Type":"text/html;charset=gbk"}
    }
  }

]